{
    "autoload": {
        "psr-4": {
            "Temant\\SettingsManager\\": "Src/"
        }
    },
    "require": {
        "doctrine/orm": "^3.2",
        "symfony/cache": "^7.1"
    },
    "require-dev": {
        "symfony/var-dumper": "^7.1",
        "phpstan/phpstan": "^1.12",
        "phpunit/phpunit": "^11.3"
    },
    "scripts": {
        "phpstan": "./vendor/bin/phpstan",
        "phpunit": "./vendor/bin/phpunit"
    },
    "scripts-aliases": {
        "phpstan": [
            "stan",
            "analyze",
            "analyse"
        ],
        "phpunit": [
            "test"
        ]
    }
}